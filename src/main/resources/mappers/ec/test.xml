<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cesco.api.cesnetapi.ec.mappers.TestMapper">
    
    <select id="getServerDateTime" resultType="com.cesco.api.cesnetapi.ec.models.ServerInfo">
        /** getServerDateTime **/
        SELECT FORMAT(GETDATE(), 'yyyy-MM-dd HH:mm:sss') AS serverDateTime
             , @@SERVERNAME AS serverName
    </select>

    <select id="getServerDateTime2" parameterType="String" statementType="CALLABLE" resultType="com.cesco.api.cesnetapi.ec.models.ServerInfo">
        {
            call BACKUPDB.dbo.Csn_GetServerDateTimeProc(
                /** getServerDateTime2 **/
                #{names}
                )
        }
    </select>

</mapper>